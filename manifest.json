{
  "manifest_version": 3,
  "name": "Cookie Editor with Whitelist",
  "version": "1.0",
  "description": "Whitelist domains, and view/edit current tab cookies.",
  "permissions": [
    "cookies",
    "tabs",
    "storage",
    "browsingData"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "incognito": "spanning",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "options_page": "options.html",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
