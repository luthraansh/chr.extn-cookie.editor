{
  "action": {
    "default_icon": {
        "128": "icon.png",
        "16": "icon.png",
        "48": "icon.png"
    },
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "description": "Whitelist domains, and view/edit current tab cookies.",
  "host_permissions": [
    "<all_urls>"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "incognito": "split",
   "key": "",
   "manifest_version": 3,
   "name": "Cookie Editor with Whitelist",
   "options_page": "options.html",
   "permissions": [ "cookies", "windows", "tabs", "activeTab", "storage", "browsingData" ],
   "version": "0.8"
}
